{
  "name": "three-aseprite",
  "type": "module",
  "version": "0.0.30",
  "description": "Three.js mesh rendering for Aseprite JSON sprite sheets",
  "keywords": [
    "three",
    "aseprite",
    "sprite"
  ],
  "author": "Robert Brownstein <robert.brownstein@gmail.com>",
  "homepage": "https://github.com/brownstein/three-aseprite/tree/main/packages/three-aseprite",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:tsc": "tsc",
    "build": "npm run build:clean && npm run build:tsc"
  },
  "dependencies": {
    "three": "^0.159.0"
  },
  "devDependencies": {
    "@types/three": "^0.159.0",
    "typescript": "^5.9.2"
  },
  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "files": [
    "src",
    "dist"
  ]
